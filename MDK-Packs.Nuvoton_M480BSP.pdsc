<boards>
  <board vendor="Nuvoton" name="NuMaker-PFM-M487KM" revision="1.0" orderForm="https://www.nuvoton.com/buy/buy-on-line/">
    <description>The NuMaker-PFM-M487KM offers M487KMCAN full pins extension connectors and Arduino UNO compatible extension connectors.</description>
    <image large="Document/NuMaker-PFM-M487KM.png" public="true"/>
    
    <book category="overview"  name="https://www.nuvoton.com/board/numaker-pfm-m487km/" title="NuMaker-PFM-M487KM Web Page"/>
    <book category="setup"     name="https://www.nuvoton.com/board/numaker-pfm-m487km/?index=2" title="Quick Start"/>
    <book category="setup"     name="Document/M480 emWin Quick Start Guide.pdf" title="emWin Quick Start Guide" public="true"/>
    <book category="schematic" name="Document/NUMAKER-PFM-M487_V3_0.pdf" title="Schematics" public="true"/>
    <book category="other"     name="Document/README.md" title="Guide" public="true"/>
    
    <mountedDevice deviceIndex="0" Dvendor="Nuvoton:18" Dname="M487JIDAE"/>
    
    <compatibleDevice deviceIndex="0" Dvendor="Nuvoton:18" Dfamily="M480"/>
    
    <feature type="ODbg"      n="1"          name="On-board Nu-LInk2"/>
    <feature type="PWR"       n="5"          name="USB Powered"/>
    <feature type="DIO"       n="128"        name="Full pins extension connectors"/>
    <feature type="MemCard"   n="1"          name="MicroSD Card slot for T-Flash"/>
    <feature type="ETH"       n="1"          name="PHY IP101GR connected RJ45 receptacle"/>
    <feature type="USB"       n="2"          name="1 x USB 2.0 High-Speed OTG/Host/Device, 1 x USB 1.1 Full-Speed OTG/Host/Device"/>
    <feature type="MIC"       n="1"          name="Audio codec (NAU88L25) with Microphone In and Headphone Out"/>
    <feature type="ConnOther" n="2"          name="Arduino UNO compatible headers"/>
    <feature type="Button"    n="2"          name="Push-buttons: Reset, 2 x User"/>
    <feature type="LED"       n="2"          name="Two user LEDs (green, red)"/>
    <feature type="CustomFF"  n="142" m="62" name="NuMaker Formfactor"/>
    
    <debugInterface adapter="NNu-Link" connector="Micro-USB"/>
  </board>
</boards>

<examples>
  <example name="Blinky" doc="README.md" folder="SampleCode/Blinky">
    <description>CMSIS-RTOS2 Blinky example with VIO</description>
    <board name="NuMaker-PFM-M487KM" vendor="Nuvoton"/>
    <project>
      <environment name="uv" load="Blinky.uvprojx"/>
    </project>
    <attributes>
      <component Cclass="Device" Cgroup="Startup"/>
      <component Cclass="CMSIS" Cgroup="CORE"/>
      <component Cclass="CMSIS" Cgroup="RTOS"/>
      <component Cclass="CMSIS Driver" Cgroup="VIO"/>
      <category>Getting Started</category>
    </attributes>
  </example>
  <example name="Platform" doc="README.md" folder="SampleCode/Platform">
    <description>Platform example</description>
    <board name="NuMaker-PFM-M487KM" vendor="Nuvoton"/>
    <project>
      <environment name="uv" load="Platform.uvprojx"/>
    </project>
    <attributes>
      <component Cclass="Device" Cgroup="Startup"/>
