<url>https://github.com/MDK-Packs/Pack/raw/master/M480BSP/</url>
<repository type="git">https://github.com/MDK-Packs/M480BSP.git</repository>
  <license>LICENSE.txt</license>
  <releases>
    <release version="0.0.1" date="2022-01-26">
      Initial Release
    </release>
  </releases>

